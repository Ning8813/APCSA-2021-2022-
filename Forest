
/**
 * Forest terrain.
 * 
 * @author Josephine Chun
 * @version 10 Jun 2022
 */
public class Forest extends Terrain
{
    private int trees;
    public Forest(int l, int w, int t)
    {
        super(l, w);
        trees = t;
    }
    
    public int getTrees()
    {
        return trees;
    }
}
