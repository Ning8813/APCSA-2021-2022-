
/**
 * Winter Mountain terrain.
 * 
 * @author Josephine Chun 
 * @version 10 Jun 2022
 */
public class WinterMountain extends Mountain
{
    private double temperature;
    public WinterMountain(int l, int w, int m, double temp)
    {
        super(l, w, m);
        temperature = temp;
    }
    
    public double getTemp()
    {
        return temperature;
    }
}
