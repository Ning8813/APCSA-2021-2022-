
/**
 * Mountainous terrain.
 * 
 * @author Josephine Chun
 * @version 10 Jun 2022
 */
public class Mountain extends Terrain
{
    private int mountains;
    public Mountain(int l, int w, int m)
    {
        super(l, w);
        mountains = m;
    }
    
    public int getMountains()
    {
        return mountains;
    }
}
